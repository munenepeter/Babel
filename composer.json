{
    "name": "tabel/framework",
    "description": "A simple simple custom framework",
    "keywords": [
        "framework",
        "tabel"
    ],
    "license": "MIT",
    "homepage": "https://github.com/munenepeter/framework",
    "support": {
        "issues": "https://github.com/munenepeter/framework/issues",
        "source": "https://github.com/munenepeter/framework"
    },
    "authors": [
        {
            "name": "munenepeter",
            "email": "munenenjega@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "illuminate/database": "^11.3"
    },
    "autoload": {
        "psr-4": {
            "Tabel\\": "src/"
        },
        "files": [
            "helpers.php"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
